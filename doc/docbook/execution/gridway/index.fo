  <fo:block id="id2601078" white-space-collapse="false" white-space-treatment="preserve" linefeed-treatment="preserve" text-align="start" wrap-option="no-wrap" space-before.minimum="0.8em" space-before.optimum="1em" space-before.maximum="1.2em" space-after.minimum="0.8em" space-after.optimum="1em" space-after.maximum="1.2em" hyphenate="false" font-family="monospace">
